{
  "name": "janmaennig/example_tests",
  "type": "typo3-cms-extension",
  "autoload": {
    "psr-4": {
      "JanMaennig\\ExampleTests\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JanMaennig\\ExampleTests\\Tests\\": "tests/"
    }
  },
  "require": {
    "typo3/cms-core": "^10.4"
  },
  "require-dev": {
    "nimut/testing-framework": "^5.1"
  },
  "scripts": {
    "post-autoload-dump": [
      "@prepare-extension-test-structure"
    ],
    "prepare-extension-test-structure": [
      "Nimut\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
    ]
  }
}
